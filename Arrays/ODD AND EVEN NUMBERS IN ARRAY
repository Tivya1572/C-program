#include <stdio.h>
int findoddeven(int arr[],int len)
{
    int i;
    for(i=0;i<len;i++)
    {
        if(arr[i]%2==0)
        printf("%d ",arr[i]);
    }
    printf("\n");
    for(i=0;i<len;i++)
    {
        if(arr[i]%2!=0)
        printf("%d ",arr[i]);
    }
}
       
int main()
{
    int arr[] = { 1,5,7,1,2,4,3,3,8,6,5,3,1,-2};
    int len;
    len = sizeof(arr)/sizeof(arr[0]);
    findoddeven(arr,len);
    
}
